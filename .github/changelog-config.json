{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": ["feature", "enhancement"]
        },
        {
            "title": "## 🐛 Bug Fixes",
            "labels": ["fix", "bug"]
        },
        {
            "title": "## 📦 Dependencies",
            "labels": ["dependencies"]
        },
        {
            "title": "## 📝 Documentation",
            "labels": ["documentation"]
        },
        {
            "title": "## 🔧 Maintenance",
            "labels": ["chore", "maintenance"]
        }
    ],
    "template": "${{CHANGELOG}}",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "empty_template": "- No changes",
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365
}
